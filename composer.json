{
   "name": "dariassoft/laravel-menu",
   "description": "Menu and sidebar management package for Laravel 11 and 12",
   "keywords": [
       "laravel",
       "menu",
       "navigation",
       "sidebar",
       "bootstrap"
   ],
   "license": "MIT",
   "authors": [
        {
            "name": "Denis Duli√ßi",
            "email": "info@akaunting.com",
            "homepage": "https://akaunting.com",
            "role": "Developer"
        },
       {
           "name": "Daniel Esteban Arias (Fork Maintainer)",
           "email": "dariassoft@gmail.com",
           "role": "Maintainer"
       }
   ],
   "require": {
        "php": "^8.3",
        "illuminate/config": "^9.0|^10.0|^11.0|^12.0",
        "illuminate/support": "^9.0|^10.0|^11.0|^12.0",
        "illuminate/view": "^9.0|^10.0|^11.0|^12.0",
        "spatie/laravel-html": "^3.0"
   },
   "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.12",
        "mockery/mockery": "^1.5|^1.6",
        "orchestra/testbench": "^7.4|^8.0|^9.0|^10.0",
        "phpunit/phpunit": "^9.5|^11.0"
   },
   "autoload": {
        "psr-4": {
            "Akaunting\\Menu\\": "src"
        },
        "files": [
            "src/helpers.php"
        ]
   },
    "autoload-dev": {
        "psr-4": {
            "Akaunting\\Menu\\Tests\\": "tests"
        }
    },
   "extra": {
        "laravel": {
            "providers": [
                "Akaunting\\Menu\\Provider"
            ],
            "aliases": {
                "Menu": "Akaunting\\Menu\\Facade"
            }
        }
   },
   "minimum-stability": "dev",
   "prefer-stable": true
}
